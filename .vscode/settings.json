{
  "files.associations": {
    "*.h": "c",
    "*.txt": "ansi"
  },
  "C_Cpp.errorSquiggles": "EnabledIfIncludesResolve",
  "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Webkit, ColumnLimit: 100 }",
  "C_Cpp.clang_format_style": "{ BasedOnStyle: Google, ColumnLimit: 100 }",
  "c-cpp-flylint.cppcheck.enable": false,
  "c-cpp-flylint.flawfinder.enable": false
}
